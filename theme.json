{
	"version": 2,
	"customTemplates": [
		{
			"name": "page-sidebar",
			"title": "Page - Sidebar",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "content-only",
			"title": "Content Only",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"templateParts": [
		{
			"title": "Header",
			"name": "header",
			"area": "header"
		},
		{
			"title": "Footer",
			"name": "footer",
			"area": "footer"
		},
		{
			"title": "Entry Meta",
			"name": "entry-meta",
			"area": "uncategorized"
		},
		{
			"title": "Main Sidebar",
			"name": "sidebar",
			"area": "uncategorized"
		}
	],
	"settings": {
		"appearanceTools": true,
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#2388ed",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#6c757d",
					"name": "Secondary"
				},
				{
					"slug": "success",
					"color": "#7dc855",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#ffc107",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "#dc3545",
					"name": "Danger"
				},
				{
					"slug": "info",
					"color": "#0dcaf0",
					"name": "Info"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "light",
					"color": "#f8f9fa",
					"name": "Light"
				},
				{
					"slug": "dark",
					"color": "#343a40",
					"name": "Dark"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-success",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--success))",
					"name": "Primary to Success"
				},
				{
					"slug": "primary-to-vivid-purple",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--vivid-purple))",
					"name": "Primary to Purple"
				},
				{
					"slug": "success-to-warning",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--success), var(--wp--preset--color--warning))",
					"name": "Success to Warning"
				},
				{
					"slug": "danger-to-warning",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--danger), var(--wp--preset--color--warning))",
					"name": "Danger to Warning"
				},
				{
					"slug": "secondary-to-light",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--secondary), var(--wp--preset--color--light))",
					"name": "Secondary to Light"
				},
				{
					"slug": "light-to-white",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--light), var(--wp--preset--color--white))",
					"name": "Light to White"
				}
			],
			"link": true,
			"custom": true,
			"customGradient": true,
			"corePalette": false,
			"coreGradients": false
		},
		"custom": {
			"breakpoints": {
				"sm": "640px",
				"md": "768px",
				"lg": "992px",
				"xl": "1200px",
				"xxl": "1440px"
			},
			"shadows": {
				"sm": "0 0.125rem 0.25rem rgba(0,0,0,.08)",
				"lg": "0 3px 1rem .25rem rgba(0,0,0,.10)"
			},
			"spacers": {
				"0": "0",
				"1": ".5rem",
				"2": ".75rem",
				"3": "1rem",
				"4": "2rem",
				"5": "3rem"
			},
			"mobileBreakpoint": "lg"
		},
		"layout": {
			"contentSize": "1320px",
			"wideSize": "1440px"
		},
		"spacing": {
			"blockGap": true,
			"customMargin": true,
			"customPadding": true,
			"units": [
				"px",
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"customFontSize": true,
			"customLineHeight": true,
			"customFontStyle": true,
			"customFontWeight": true,
			"customTextTransforms": true,
			"customTextDecorations": true,
			"fontFamilies": [
				{
					"fontFamily": "var(--wp--font-sans-serif)",
					"slug": "base",
					"name": "Base"
				},
				{
					"fontFamily": "var(--wp--font-monospace)",
					"slug": "monospace",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": ".8rem",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "1rem",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "1.3rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "huge",
					"size": "2rem",
					"name": "Huge"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/post-author": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"core/post-comments-link": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			},
			"core/quote": {
				"typography": {
					"fontSize": "var:preset|font-size|normal"
				}
			},
			"core/button": {
				"typography": {
					"fontFamily": "var:preset|font-family|button",
					"fontSize": "var:preset|font-size|normal"
				},
				"border": {
					"width": "1px",
					"radius": "0.25rem"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "1rem"
				}
			},
			"core/columns": {
				"spacing": {
					"padding": {
						"left": "1rem",
						"right": "1rem"
					}
				}
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|primary"
				}
			}
		},
		"color": {
			"background": "var:preset|color|white",
			"text": "var:preset|color|dark"
		},
		"spacing": {
			"blockGap": null
		},
		"typography": {
			"fontSize": "var:preset|font-size|normal",
			"fontFamily": "var:preset|font-family|base",
			"lineHeight": "1.5"
		}
	}
}