{
	"name": "wecodeart",
	"version": "6.4.5",
	"main": "webpack.config.js",
	"author": "Bican Marian Valeriu",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"build": "wp-scripts build frontend=./src/js/frontend/index.js --output-path=assets/minified",
		"start": "wp-scripts start frontend=./src/js/frontend/index.js --output-path=assets/unminified",
		"start:hot": "wp-scripts start frontend=./src/js/frontend/index.js --output-path=assets/unminified --hot",
		"build:admin": "wp-scripts build admin/admin=./src/js/admin/index.js --output-path=assets/minified",
		"start:admin": "wp-scripts start admin/admin=./src/js/admin/index.js --output-path=assets/unminified",
		"build:plugin:focustrap": "wp-scripts build plugins/focustrap=./src/js/frontend/plugins/Plugin-FocusTrap.js --output-path=assets/minified",
		"start:plugin:focustrap": "wp-scripts start plugins/focustrap=./src/js/frontend/plugins/Plugin-FocusTrap.js --output-path=assets/unminified",
		"build:plugin:backdrop": "wp-scripts build plugins/backdrop=./src/js/frontend/plugins/Plugin-Backdrop.js --output-path=assets/minified",
		"start:plugin:backdrop": "wp-scripts start plugins/backdrop=./src/js/frontend/plugins/Plugin-Backdrop.js --output-path=assets/unminified",
		"build:plugin:template": "wp-scripts build plugins/template=./src/js/frontend/plugins/Plugin-Template.js --output-path=assets/minified",
		"start:plugin:template": "wp-scripts start plugins/template=./src/js/frontend/plugins/Plugin-Template.js --output-path=assets/unminified",
		"build:plugin:toast": "wp-scripts build plugins/toast=./src/js/frontend/plugins/Plugin-Toast.js --output-path=assets/minified",
		"start:plugin:toast": "wp-scripts start plugins/toast=./src/js/frontend/plugins/Plugin-Toast.js --output-path=assets/unminified",
		"build:modules:dropdown": "wp-scripts build modules/dropdown=./src/js/frontend/modules/dropdown.js --output-path=assets/minified",
		"start:modules:dropdown": "wp-scripts start modules/dropdown=./src/js/frontend/modules/dropdown.js --output-path=assets/unminified",
		"build:admin:plugins": "wp-scripts build admin/plugins=./src/js/admin/plugins/index.js --output-path=assets/minified",
		"start:admin:plugins": "wp-scripts start admin/plugins=./src/js/admin/plugins/index.js --output-path=assets/unminified",
		"build:admin:themes": "wp-scripts build admin/themes=./src/js/admin/themes/index.js --output-path=assets/minified",
		"start:admin:themes": "wp-scripts start admin/themes=./src/js/admin/themes/index.js --output-path=assets/unminified",
		"build:admin:notifications": "wp-scripts build admin/notifications=./src/js/admin/notifications/index.js --output-path=assets/minified",
		"start:admin:notifications": "wp-scripts start admin/notifications=./src/js/admin/notifications/index.js --output-path=assets/unminified",
		"build:gutenberg": "wp-scripts build gutenberg/editor=./src/js/gutenberg/index.js --output-path=assets/minified",
		"start:gutenberg": "wp-scripts start gutenberg/editor=./src/js/gutenberg/index.js --output-path=assets/unminified",
		"start:gutenberg:patterns": "wp-scripts start gutenberg/ext-patterns=./src/js/gutenberg/extensions/with-patterns/index.js --output-path=assets/unminified",
		"build:gutenberg:patterns": "wp-scripts build gutenberg/ext-patterns=./src/js/gutenberg/extensions/with-patterns/index.js --output-path=assets/minified",
		"start:gutenberg:styles": "wp-scripts start gutenberg/ext-styles=./src/js/gutenberg/extensions/with-styles/index.js --output-path=assets/unminified",
		"build:gutenberg:styles": "wp-scripts build gutenberg/ext-styles=./src/js/gutenberg/extensions/with-styles/index.js --output-path=assets/minified",
		"start:gutenberg:classes": "wp-scripts start gutenberg/ext-classes=./src/js/gutenberg/extensions/with-classes/index.js --output-path=assets/unminified",
		"build:gutenberg:classes": "wp-scripts build gutenberg/ext-classes=./src/js/gutenberg/extensions/with-classes/index.js --output-path=assets/minified",
		"start:modules:formatting": "wp-scripts start index=./inc/support/modules/formatting/src/js/index.js --output-path=inc/support/modules/formatting/assets/unminified",
		"build:modules:formatting": "wp-scripts build index=./inc/support/modules/formatting/src/js/index.js --output-path=inc/support/modules/formatting/assets/minified",
		"start:modules:lorem": "wp-scripts start index=./inc/support/modules/lorem/src/js/index.js --output-path=inc/support/modules/lorem/assets/unminified",
		"build:modules:lorem": "wp-scripts build index=./inc/support/modules/lorem/src/js/index.js --output-path=inc/support/modules/lorem/assets/minified",
		"start:modules:accordion": "wp-scripts start index=./inc/support/modules/accordion/src/js/index.js --output-path=inc/support/modules/accordion/assets/unminified",
		"build:modules:accordion": "wp-scripts build index=./inc/support/modules/accordion/src/js/index.js --output-path=inc/support/modules/accordion/assets/minified",
		"start:modules:scrolltop": "wp-scripts start admin=./inc/support/modules/scrolltop/src/js/admin.js --output-path=inc/support/modules/scrolltop/assets/unminified",
		"build:modules:scrolltop": "wp-scripts build admin=./inc/support/modules/scrolltop/src/js/admin.js --output-path=inc/support/modules/scrolltop/assets/minified",
		"start:modules:optimization": "wp-scripts start admin=./inc/support/modules/optimization/src/js/admin.js --output-path=inc/support/modules/optimization/assets/unminified",
		"build:modules:optimization": "wp-scripts build admin=./inc/support/modules/optimization/src/js/admin.js --output-path=inc/support/modules/optimization/assets/minified",
		"start:modules:contactform": "wp-scripts start admin=./inc/support/modules/contactform/src/js/admin.js --output-path=inc/support/modules/contactform/assets/unminified",
		"build:modules:contactform": "wp-scripts build admin=./inc/support/modules/contactform/src/js/admin.js --output-path=inc/support/modules/contactform/assets/minified",
		"lint:css": "wp-scripts lint-style ./src/scss",
		"lint:js": "wp-scripts lint-js ./src/js/routes ./src/js/frontend.js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"dependencies": {
		"@wordpress/browserslist-config": "^5.38.0",
		"@wordpress/icons": "^9.2.0",
		"@wordpress/interactivity": "^5.0.0",
		"@wordpress/scripts": "^27.2.0",
		"bootstrap": "^5.1.3",
		"classnames": "^2.2.6",
		"dploy": "^1.2.0",
		"loadjs": "^4.3.0-rc1",
		"react": "^18.2.0",
		"react-lorem-ipsum": "^1.4.9"
	}
}
