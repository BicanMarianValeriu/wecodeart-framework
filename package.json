{
	"name": "wecodeart",
	"version": "4.1.7",
	"main": "webpack.config.js",
	"author": "Bican Marian Valeriu",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"build": "wp-scripts build",
		"start": "wp-scripts start",
		"build:admin:css": "wp-scripts build ./src/scss/admin/index.scss",
		"start:admin:css": "wp-scripts start ./src/scss/admin/index.scss",
		"build:admin:notification": "wp-scripts build ./src/js/admin/notifications/index.js --output-path=assets/minified",
		"start:admin:notification": "wp-scripts start ./src/js/admin/notifications/index.js --output-path=assets/unminified",
		"build:gutenberg": "wp-scripts build ./src/js/gutenberg/index.js --output-path=assets/minified/js/gutenberg",
		"start:gutenberg": "wp-scripts start ./src/js/gutenberg/index.js --output-path=assets/unminified/js/gutenberg",
		"build:gutenberg:css": "wp-scripts build ./src/scss/gutenberg/index.scss --output-path=assets/minified/css/gutenberg",
		"start:gutenberg:css": "wp-scripts start ./src/scss/gutenberg/index.scss --output-path=assets/unminified/css/gutenberg",
		"build:customizer": "wp-scripts build ./src/js/customizer/index.js --output-path=assets/minified",
		"start:customizer": "wp-scripts start ./src/js/customizer/index.js --output-path=assets/unminified",
		"build:customizer:slider": "wp-scripts build ./src/js/customizer/controls/slider/index.js --output-path=assets/minified",
		"start:customizer:slider": "wp-scripts start ./src/js/customizer/controls/slider/index.js --output-path=assets/unminified",
		"build:customizer:sortable": "wp-scripts build ./src/js/customizer/controls/sortable/index.js --output-path=assets/minified",
		"start:customizer:sortable": "wp-scripts start ./src/js/customizer/controls/sortable/index.js --output-path=assets/unminified",
		"build:customizer:css": "wp-scripts build ./src/scss/customizer/index.scss",
		"start:customizer:css": "wp-scripts start ./src/scss/customizer/index.scss",
		"lint:css": "wp-scripts lint-style ./src/scss",
		"lint:js": "wp-scripts lint-js ./src/js/routes ./src/js/frontend.js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.10.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.29",
		"@wordpress/browserslist-config": "^2.6.0",
		"@wordpress/scripts": "^10.0.0",
		"autoprefixer": "^9.8.1",
		"babel-eslint": "^10.1.0",
		"bootstrap": "^4.5.0",
		"browser-sync": "^2.26.7",
		"browser-sync-webpack-plugin": "^2.2.2",
		"classnames": "^2.2.6",
		"css-loader": "^3.6.0",
		"eslint": "^7.3.0",
		"file-loader": "^6.0.0",
		"ignore-emit-webpack-plugin": "^2.0.2",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"popper.js": "^1.16.1",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"react": "^16.13.1",
		"react-lorem-ipsum": "^1.4.4",
		"sass-loader": "^8.0.2"
	}
}
