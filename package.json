{
	"name": "wecodeart",
	"version": "5.0.0",
	"main": "webpack.config.js",
	"author": "Bican Marian Valeriu",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"build": "wp-scripts build",
		"start": "wp-scripts start",
		"build:admin": "wp-scripts build ./src/js/admin/admin.js --output-path=assets/minified",
		"start:admin": "wp-scripts start ./src/js/admin/admin.js --output-path=assets/unminified",
		"build:admin:notification": "wp-scripts build ./src/js/admin/admin-notification.js --output-path=assets/minified",
		"start:admin:notification": "wp-scripts start ./src/js/admin/admin-notification.js --output-path=assets/unminified",
		"build:gutenberg": "wp-scripts build ./src/js/gutenberg/gutenberg.js --output-path=assets/minified",
		"start:gutenberg": "wp-scripts start ./src/js/gutenberg/gutenberg.js --output-path=assets/unminified",
		"start:gutenberg:patterns": "wp-scripts start ./src/js/gutenberg/extensions/with-patterns/gutenberg-patterns.js --output-path=assets/unminified",
		"build:gutenberg:patterns": "wp-scripts build ./src/js/gutenberg/extensions/with-patterns/gutenberg-patterns.js --output-path=assets/minified",
		"start:gutenberg:styles": "wp-scripts start ./src/js/gutenberg/extensions/with-styles/gutenberg-styles.js --output-path=assets/unminified",
		"build:gutenberg:styles": "wp-scripts build ./src/js/gutenberg/extensions/with-styles/gutenberg-styles.js --output-path=assets/minified",
		"start:gutenberg:classes": "wp-scripts start ./src/js/gutenberg/extensions/with-classes/gutenberg-classes.js --output-path=assets/unminified",
		"build:gutenberg:classes": "wp-scripts build ./src/js/gutenberg/extensions/with-classes/gutenberg-classes.js --output-path=assets/minified",
		"build:customizer": "wp-scripts build ./src/js/customizer/customizer.js --output-path=assets/minified",
		"start:customizer": "wp-scripts start ./src/js/customizer/customizer.js --output-path=assets/unminified",
		"build:customizer:range": "wp-scripts build ./src/js/customizer/controls/customizer-range.js --output-path=assets/minified",
		"start:customizer:range": "wp-scripts start ./src/js/customizer/controls/customizer-range.js --output-path=assets/unminified",
		"build:customizer:sortable": "wp-scripts build ./src/js/customizer/controls/customizer-sortable.js --output-path=assets/minified",
		"start:customizer:sortable": "wp-scripts start ./src/js/customizer/controls/customizer-sortable.js --output-path=assets/unminified",
		"start:customizer:fonts": "wp-scripts start ./src/js/customizer/controls/customizer-fonts.js --output-path=assets/unminified",
		"build:customizer:fonts": "wp-scripts build ./src/js/customizer/controls/customizer-fonts.js --output-path=assets/minified",
		"start:customizer:color": "wp-scripts start ./src/js/customizer/controls/customizer-color.js --output-path=assets/unminified",
		"build:customizer:color": "wp-scripts build ./src/js/customizer/controls/customizer-color.js --output-path=assets/minified",
		"start:customizer:palette": "wp-scripts start ./src/js/customizer/controls/customizer-palette.js --output-path=assets/unminified",
		"build:customizer:palette": "wp-scripts build ./src/js/customizer/controls/customizer-palette.js --output-path=assets/minified",
		"start:plugin:cf7": "wp-scripts start ./src/js/plugins/cf7/support-cf7.js --output-path=assets/unminified",
		"build:plugin:cf7": "wp-scripts build ./src/js/plugins/cf7/support-cf7.js --output-path=assets/minified",
		"lint:css": "wp-scripts lint-style ./src/scss",
		"lint:js": "wp-scripts lint-js ./src/js/routes ./src/js/frontend.js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@popperjs/core": "^2.6.0",
		"@wordpress/browserslist-config": "^4.0.0",
		"@wordpress/icons": "^4.0.0",
		"@wordpress/scripts": "^16.1.0",
		"array-move": "^3.0.1",
		"bootstrap": "^5.0.1",
		"browser-sync": "^2.26.14",
		"browser-sync-webpack-plugin": "^2.3.0",
		"classnames": "^2.2.6",
		"dploy": "^1.2.0",
		"holderjs": "^2.9.9",
		"react": "^17.0.1",
		"react-lorem-ipsum": "^1.4.9",
		"react-sortable-hoc": "^2.0.0",
		"react-visibility-sensor": "^5.1.1"
	}
}
